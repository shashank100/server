{
    "presets": [
      "react",
      "env",
      "stage-2"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-export-default",
      "transform-class-properties",
      [
        "transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
}
